        "entity.name",
        "entity.other.inherited-class",
        "markup.changed.git_gutter",
        "meta.use",
        "storage.type.annotation",
        "storage.type.java",
        "storage.modifier.import.java",
        "storage.modifier.package.java",
        "support.class",
        "support.other.namespace.use.php",
        "support.other.namespace.php",
