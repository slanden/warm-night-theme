{
  "name": "Warm Night",
  "variables": {
    // Global
    "--surface1": "hsl(182.2, 93.1%, 5.7%)",
    "--surface2": "hsl(180, 24.5%, 10.4%)",
    "--surface3": "hsl(180, 18.3%, 13.9%)",
    "--surface3-border": "hsl(180, 18.3%, 17%)",
    "--surface4": "hsl(180, 9.1%, 12.9%)",
    "--surface5": "hsl(180, 7.5%, 15.7%)",
    "--surface6": "hsl(180, 6.3%, 19%)",
    "--text1": "hsl(40.5, 29%, 72.9%)",
    "--text2": "hsl(40.5, 35%, 80%)",
    // "--interact2": "#4c928b34",
    // "--interact2-brighter": "#4c928b42",
    "--error": "hsl(0, 80%, 60%)",
    "--highlight": "hsl(12, 83%, 66%, 0.25)",
    // "--highlight-inactive": "hsl(12, 83%, 66%, 0.15)",
    "--highlight-inactive": "hsl(12, 83%, 66%, 0.15)",
    // "--border1": "hsl(13.1, 15.8%, 39.6%)",
    // "--border2": "hsl(180, 18.3%, 13.9%)",
    "--status-bar": "hsl(222.9, 14.9%, 18.4%)",
    "--tab-inactive-fg": "hsl(18, 11.2%, 65.1%)",
    "--bracket2": "hsl(41.2, 22.9%, 58.8%)",
    "--bracket3": "hsl(31.2, 19.8%, 49.4%)",
    "--bracket4": "hsl(8.1, 22.2%, 54.1%)",
    "--codelens-text": "hsl(0, 6.5%, 36.5%)",
    "--md-h1": "hsl(12.2, 100%, 75%)",
    "--md-h2": "hsl(11.7, 74.5%, 67.6%)",
    "--md-h3": "hsl(11.5, 50%, 56%)",
    "--md-h4": "hsl(11.8, 45%, 51%)",
    "--md-h5": "hsl(12, 40%, 47%)",
    "--md-h6": "hsl(12.5, 35%, 43%)",
    "--md-h2-txt": "hsl(341.6, 70%, 63.5%, .95)",
    "--md-h3-txt": "hsl(341.6, 65%, 63.5%, .95)",
    "--md-h4-txt": "hsl(341.6, 60%, 63.5%, .9)",
    "--md-h6-txt": "hsl(341.6, 50%, 63.5%, .85)",
    // Language
    "--keyword": "hsl(12.1, 82.8%, 65.9%)",
    "--variable": "hsl(20, 56.6%, 72%)",
    "--punctuation": "hsl(20, 28.2%, 48.6%)",
    "--comment1": "hsl(339, 11%, 53%)",
    "--comment2": "hsl(339, 11%, 62%)",
    "--comment-brighter": "hsl(339, 15%, 73%)",
    "--string": "hsl(25, 69%, 60%)",
    "--string2": "hsl(25, 69%, 70%)",
    "--number": "hsl(90, 23.3%, 59.6%)",
    // "--constant-warm": "hsl(36.6, 100%, 83%)",
    "--constant-warm": "hsl(34, 66%, 74%)",
    // "--constant-cold": "hsl(265, 15%, 69%)",
    "--constant-cold": "hsl(265, 12%, 68%)",
    // "--function": "hsl(33.8, 100%, 69%)",
    "--function": "hsl(31, 93%, 67%)",
    // For hover color when used on interactive elements
    "--function-brighter": "hsl(31, 91%, 72%)",
    "--entity": "hsl(341.6, 75.3%, 63.5%)",
    "--operator": "hsl(174, 31.5%, 43.5%)"
    // "--attribute": "hsl(20, 56.6%, 72%)",
  },
  "colors": {
    "activityBar.background": "var(--surface3)",
    "activityBar.foreground": "var(--keyword)",
    "activityBarBadge.background": "var(--function)",
    "activityBarBadge.foreground": "var(--surface1)",
    "badge.background": "var(--function)",
    "badge.foreground": "var(--surface1)",
    "banner.iconForeground": "var(--keyword)",
    // "banner.background": "#b14a389d",
    "banner.background": "var(--keyword)",
    "banner.foreground": "var(--surface1)",
    "breadcrumb.activeSelectionForeground": "var(--text2)",
    "breadcrumb.focusForeground": "var(--text2)",
    "button.background": "var(--function)",
    "button.border": "var(--function)",
    "button.foreground": "var(--surface1)",
    "button.hoverBackground": "var(--function-brighter)",
    "button.secondaryBackground": "var(--surface5)",
    "button.secondaryForeground": "var(--text1)",
    "button.secondaryHoverBackground": "var(--surface6)",
    "editor.background": "var(--surface1)",
    "editor.findMatchBackground": "var(--highlight)",
    "editor.findMatchHighlightBackground": "var(--highlight-inactive)",
    // "editor.foldBackground": "#43616434",
    "editor.foreground": "var(--text1)",
    "editor.hoverHighlightBackground": "var(--highlight-inactive)",
    "editor.lineHighlightBorder": "var(--surface3-border)",
    "editor.selectionBackground": "var(--highlight)",
    "editor.wordHighlightBackground": "var(--highlight)",
    "editor.wordHighlightStrongBackground": "var(--highlight)",
    "editorCursor.foreground": "var(--text1)",
    "editorBracketMatch.border": "var(--bracket3)",
    // "editorBracketHighlight.foreground1": "#ffca85",
    "editorBracketHighlight.foreground1": "var(--text1)",
    // "editorBracketHighlight.foreground2": "#e3b578",
    "editorBracketHighlight.foreground2": "var(--bracket2)",
    // "editorBracketHighlight.foreground3": "#D9BE9A",
    "editorBracketHighlight.foreground3": "var(--bracket3)",
    // "editorBracketHighlight.foreground4": "#A68B68",
    "editorBracketHighlight.foreground4": "var(--bracket4)",
    // "editorBracketHighlight.foreground5": "#B3AFAA",
    "editorBracketHighlight.foreground5": "var(--text1)",
    // "editorBracketHighlight.foreground6": "#A68963",
    "editorBracketHighlight.foreground6": "var(--bracket2)",
    "editorBracketHighlight.unexpectedBracket.foreground": "var(--keyword)",
    "editorBracketPairGuide.activeBackground1": "var(--punctuation)",
    "editorBracketPairGuide.activeBackground2": "var(--punctuation)",
    "editorBracketPairGuide.activeBackground3": "var(--punctuation)",
    "editorBracketPairGuide.activeBackground4": "var(--punctuation)",
    "editorBracketPairGuide.activeBackground5": "var(--punctuation)",
    "editorBracketPairGuide.activeBackground6": "var(--punctuation)",
    // "editorCodeLens.foreground": "var(--codelens-text)",
    "editorError.foreground": "var(--error)",
    "editorGroupHeader.tabsBackground": "var(--surface2)",
    "editorInlayHint.background": "#0000",
    "editorInlayHint.foreground": "var(--comment1)",
    "editorLineNumber.activeForeground": "var(--comment-brighter)",
    "editorLineNumber.foreground": "var(--comment1)",
    "editorLink.activeForeground": "var(--function-brighter)",
    // "editorSuggestWidget.background": "var(--surface4)",
    "editorSuggestWidget.focusHighlightForeground": "var(--function)",
    "editorSuggestWidget.highlightForeground": "var(--function)",
    "editorSuggestWidget.selectedBackground": "var(--highlight)",
    // "editorSuggestWidget.selectedForeground": "var(--text1)",
    // "editorWarning.foreground": "var(--function-brighter)",
    "editorWidget.background": "var(--surface4)",
    // ?
    // Not sure if this should instead be the same color
    // as other interactions, or perhaps the same as
    // accents, which use `--keyword`.
    "extensionIcon.sponsorForeground": "var(--operator)",
    "focusBorder": "var(--function)",
    "foreground": "var(--text1)",
    "icon.foreground": "var(--text1)",
    "list.activeSelectionBackground": "var(--highlight)",
    "list.activeSelectionForeground": "var(--text1)",
    "list.focusHighlightForeground": "var(--function)",
    "list.highlightForeground": "var(--function)",
    "list.inactiveSelectionBackground": "var(--highlight-inactive)",
    "notifications.background": "var(--surface4)",
    // For some reason, this doesn't affect the top right icons
    // "notifications.foreground": "var(--surface1)",
    // "panelTitle.activeBorder": "var(--text1)",
    "panelTitle.activeForeground": "var(--text1)",
    "peekView.border": "var(--function)",
    "peekViewEditor.background": "var(--surface4)",
    "peekViewEditor.matchHighlightBackground": "var(--highlight)",
    "peekViewEditorGutter.background": "var(--surface4)",
    "peekViewResult.background": "var(--surface5)",
    "peekViewResult.fileForeground": "var(--text2)",
    "peekViewResult.lineForeground": "var(--text1)",
    // "peekViewResult.matchHighlightBackground": "var(--highlight)",
    "peekViewTitleDescription.foreground": "var(--text1)",
    "peekViewTitleLabel.foreground": "var(--text2)",
    // "pickerGroup.foreground": "#fc905b", OR '--keyword'
    "pickerGroup.foreground": "var(--function)",
    "progressBar.background": "var(--function)",
    "selection.background": "var(--highlight)",
    "sideBar.background": "var(--surface2)",
    "sideBarSectionHeader.background": "var(--surface2)",
    "sideBarTitle.foreground": "var(--text1)",
    "statusBar.background": "var(--status-bar)",
    "statusBar.foreground": "var(--text1)",
    "statusBar.noFolderBackground": "var(--status-bar)",
    "symbolIcon.classForeground": "var(--entity)",
    "symbolIcon.constantForeground": "var(--constant-warm)",
    "symbolIcon.fieldForeground": "var(--variable)",
    "symbolIcon.functionForeground": "var(--function)",
    "symbolIcon.methodForeground": "var(--function)",
    "symbolIcon.propertyForeground": "var(--variable)",
    "symbolIcon.variableForeground": "var(--variable)",
    "tab.activeForeground": "var(--text1)",
    "tab.inactiveBackground": "var(--surface3)",
    // "tab.inactiveForeground": "var(--tab-inactive-bg)",
    "terminal.foreground": "var(--text1)",
    "terminal.selectionBackground": "var(--highlight)",
    "textBlockQuote.border": "var(--keyword)",
    "textLink.activeForeground": "var(--function-brighter)",
    "textLink.foreground": "var(--function)",
    "titleBar.activeBackground": "var(--surface4)"
    // "inputValidation.infoBorder": "#00ff00",
    // "editor.findRangeHighlightBackground": "#3a3d4166",
    // "inputOption.activeBackground": "#fc905b58",
    // "menu.selectionBackground": "#fc905b58",
    // "menubar.selectionBackground": "#ffffff1a",
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "var(--comment1)"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.parameter",
        "string constant.other.placeholder",
        // Block level variables
        "meta.block variable.other",
        // Other
        "meta.function.call.rust",
        "meta.object.member",
        "support.other.variable",
        "support.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "var(--variable)"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name",
        // JSON properties
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "var(--variable)"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag"
      ],
      "settings": {
        "foreground": "var(--variable)"
      }
    },
    {
      // These don't seem to have an effect
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "var(--variable)"
      }
    },
    {
      "name": "Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        // "variable.parameter",
        // "keyword.other.unit",
        "constant.other.caps",
        "constant.other.php",
        "variable.other.constant"
        // "variable.other.enummember",
        // "keyword.other"
      ],
      "settings": {
        "foreground": "var(--constant-warm)"
      }
    },
    {
      "name": "CSS ID's",
      "scope": [
        "source.sass keyword.control"
      ],
      "settings": {
        "foreground": "var(--constant-warm)"
      }
    },
    {
      "name": "Other Constant",
      "scope": [
        "source.css support.constant",
        "support.constant.property-value",
        // "support.constant.color.w3c-extended-color-name.css"
        "constant.other.color",
        // "meta.property-list.css",
        "meta.property-value.css"
        // "constant.language.json"
      ],
      "settings": {
        "foreground": "var(--constant-cold)"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "var(--error)"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "var(--keyword)"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter",
        "variable.language"
      ],
      "settings": {
        "foreground": "var(--keyword)"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        // "keyword.control",
        // "constant.other.color",
        "punctuation",
        // "meta.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        // "punctuation.definition.template-expression",
        // "source.js expression punctuation.definition.template-expression",
        // "meta.template.expression punctuation.definition.template-expression",
        // "punctuation.section.embedded",
        // "punctuation.definition.template-expression",
        // "meta.template.expression",
        // "string.template",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "var(--punctuation)"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "var(--operator)"
      }
    },
    {
      "name": "Operators, Preprocessors",
      "scope": [
        // "entity.name.function.preprocessor",
        "punctuation.section.embedded",
        // "punctuation.definition.template-expression.begin",
        // "punctuation.definition.template-expression.end",
        "keyword.operator",
        "punctuation.definition.interpolation"
        // "expression.begin.js"
      ],
      "settings": {
        "foreground": "var(--operator)"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "var(--function)"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "var(--function)"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": [
        "entity.name.method.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "var(--function)"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "var(--function)"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "var(--number)"
      }
    },
    {
      "name": "String, Symbols, Inherited Class",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "var(--string)"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "var(--string2)"
      }
    },
    {
      "name": "Entity Types, Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "var(--entity)"
      }
    },
    {
      "name": "CSS Classes",
      "scope": [
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "var(--entity)"
      }
    },
    {
      "name": "Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": [
        "text.html.markdown markup.inline.raw.markdown",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "var(--string2)"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "heading.1.markdown punctuation.definition.heading"
      ],
      "settings": {
        "foreground": "var(--md-h1)"
      }
    },
    {
      "name": "Markdown - Heading 2",
      "scope": [
        "heading.2.markdown punctuation.definition.heading"
      ],
      "settings": {
        "foreground": "var(--md-h2)"
      }
    },
    {
      "name": "Markdown - Heading 3",
      "scope": [
        "heading.3.markdown punctuation.definition.heading"
      ],
      "settings": {
        "foreground": "var(--md-h3)"
      }
    },
    {
      "name": "Markdown - Heading 4",
      "scope": [
        "heading.4.markdown punctuation.definition.heading"
      ],
      "settings": {
        "foreground": "var(--md-h4)"
      }
    },
    {
      "name": "Markdown - Heading 5",
      "scope": [
        "heading.5.markdown punctuation.definition.heading"
      ],
      "settings": {
        "foreground": "var(--md-h5)"
      }
    },
    {
      "name": "Markdown - Heading 6",
      "scope": [
        "heading.6.markdown punctuation.definition.heading"
      ],
      "settings": {
        "foreground": "var(--md-h6)"
      }
    },
    {
      "name": "Markdown - Heading Entity 1",
      "scope": [
        "heading.1.markdown entity.name"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Heading Entity 2",
      "scope": [
        "heading.2.markdown entity.name"
      ],
      "settings": {
        "foreground": "var(--md-h3-txt)",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Heading Entity 4",
      "scope": [
        "heading.4.markdown entity.name"
      ],
      "settings": {
        "foreground": "var(--md-h2-txt)"
      }
    },
    {
      "name": "Markdown - Heading Entity 5",
      "scope": [
        "heading.5.markdown entity.name"
      ],
      "settings": {
        "foreground": "var(--md-h4-txt)"
      }
    },
    {
      "name": "Markdown - Heading Entity 6",
      "scope": [
        "heading.6.markdown entity.name"
      ],
      "settings": {
        "foreground": "var(--md-h6-txt)"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": [
        "markup.bold",
        "markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": [
        "meta.separator"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "var(--operator)"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": [
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "var(--variable)"
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": [
        "string.other.link.description.title.markdown"
      ],
      "settings": {
        "foreground": "var(--text1)"
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": [
        "constant.other.reference.link.markdown"
      ],
      "settings": {
        "foreground": "var(--constant-warm)"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "var(--function)"
      }
    }
  ]
}